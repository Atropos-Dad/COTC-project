{
  "server": {
    "host": "0.0.0.0",
    "port": 5000
  },
  "logging_config": {
    "console_output": {
      "enabled": true,
      "level": "INFO",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
      "date_format": "%Y-%m-%d %H:%M:%S"
    },
    "file_output": {
      "enabled": true,
      "level": "DEBUG",
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
      "date_format": "%Y-%m-%d %H:%M:%S",
      "log_dir": "logs",
      "filename": "data_aggregator.log",
      "max_bytes": 10485760,
      "backup_count": 5
    }
  },
  "dashboard": {
    "update_interval_seconds": 5.0
  }
}
